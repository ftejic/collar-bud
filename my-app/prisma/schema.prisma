generator client {
  provider      = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ProductType {
  cat
  dog
}

model Product {
  id Int @id @default(autoincrement())
  type ProductType
  name String
  price Decimal @db.Decimal(5, 2)
  itemsLeft Int
}
generator client {
  provider      = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ProductCategory {
  cat
  dog
}

model Product {
  id Int @id @default(autoincrement())
  category ProductCategory
  name String
  price Decimal @db.Decimal(5, 2)
  sizes Json
  images String[]
}